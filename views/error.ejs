<%- include('./partials/head', { pageTitle: 'Something went wrong — BookLantern' }) %>
<body>
  <%- include('./partials/navbar') %>
  <main class="container page-pad">
    <div class="panel">
      <h1>Something went wrong</h1>
      <p>We couldn’t complete your request. Please try again.</p>
      <% if (typeof message !== 'undefined') { %>
        <pre style="white-space:pre-wrap;color:#b7c0d6"><%= message %></pre>
      <% } %>
    </div>
  </main>
  <%- include('./partials/footer') %>
</body>
</html>
